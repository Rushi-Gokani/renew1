{% doc %}
  @prompt
    Make a testimonials section on a deep‑green background with full‑width wavy edges at the top and bottom; center five gold star icons, a large H1 reading “Join 1,000,000+ Customers Who Love Rheal,” and a short subheading; below, add a horizontally scrollable carousel of testimonial cards (at least 5) with equal height, rounded light‑green cards and soft shadow, each showing a quote, customer name, and a “Verified Customer” label with a check icon; use 24px gaps, scroll‑snap to each card, and left/right circular arrow buttons positioned mid‑height over the carousel; add a small rating strip beneath reading “Excellent” with star icons and total reviews; desktop: show 5 cards side by side within a centered max‑width; tablet: show 2–3 per view; mobile: keep the wavy edges, show exactly one card centered and visually highlighted, and allow navigation by the arrow buttons and thumb swipes., IT'S NOT OPTIMIZED FOR MOBILE, THE HEADLINE NEEDS TO BE CENTER ALIGNED AND COMPLETELY FIT ON THE SCREEN, SHOULDNT OVERFLOW
{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-testimonials-section-{{ ai_gen_id }} {
    position: relative;
    background-color: {{ block.settings.background_color }};
    padding: 80px 0;
    overflow: hidden;
  }

  .ai-testimonials-wave-top-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 60px;
    background: {{ block.settings.background_color }};
    z-index: 1;
  }

  .ai-testimonials-wave-top-{{ ai_gen_id }}::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: {{ block.settings.background_color }};
    clip-path: polygon(0 0, 100% 0, 100% 40%, 90% 50%, 80% 60%, 70% 50%, 60% 40%, 50% 50%, 40% 60%, 30% 50%, 20% 40%, 10% 50%, 0 40%);
  }

  .ai-testimonials-wave-bottom-{{ ai_gen_id }} {
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 60px;
    background: {{ block.settings.background_color }};
    z-index: 1;
  }

  .ai-testimonials-wave-bottom-{{ ai_gen_id }}::before {
    content: '';
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: {{ block.settings.background_color }};
    clip-path: polygon(0 60%, 10% 50%, 20% 60%, 30% 50%, 40% 40%, 50% 50%, 60% 60%, 70% 50%, 80% 40%, 90% 50%, 100% 60%, 100% 100%, 0 100%);
  }

  .ai-testimonials-container-{{ ai_gen_id }} {
    max-width: 1200px;
    margin: 0 auto;
    padding: 0 20px;
    position: relative;
    z-index: 2;
  }

  .ai-testimonials-header-{{ ai_gen_id }} {
    text-align: center;
    margin-bottom: 60px;
  }

  .ai-testimonials-stars-{{ ai_gen_id }} {
    display: flex;
    justify-content: center;
    gap: 8px;
    margin-bottom: 24px;
  }

  .ai-testimonials-star-{{ ai_gen_id }} {
    width: 24px;
    height: 24px;
    color: {{ block.settings.star_color }};
  }

  .ai-testimonials-title-{{ ai_gen_id }} {
    font-family: {{ settings.type_heading_font.family }}, {{ settings.type_heading_font.fallback_families }};
    font-size: {{ block.settings.title_size }}px;
    color: {{ block.settings.text_color }};
    margin: 0 0 16px;
    line-height: 1.1;
    font-weight: normal;
    text-align: center;
    word-wrap: break-word;
    overflow-wrap: break-word;
    hyphens: auto;
  }

  .ai-testimonials-subtitle-{{ ai_gen_id }} {
    font-family: {{ settings.type_body_font.family }}, {{ settings.type_body_font.fallback_families }};
    font-size: {{ block.settings.subtitle_size }}px;
    color: {{ block.settings.text_color }};
    margin: 0;
    opacity: 0.9;
    text-align: center;
  }

  .ai-testimonials-carousel-wrapper-{{ ai_gen_id }} {
    position: relative;
    margin-bottom: 40px;
  }

  .ai-testimonials-carousel-{{ ai_gen_id }} {
    display: flex;
    gap: 24px;
    overflow-x: auto;
    scroll-snap-type: x mandatory;
    scrollbar-width: none;
    -ms-overflow-style: none;
    padding: 0 60px;
  }

  .ai-testimonials-carousel-{{ ai_gen_id }}::-webkit-scrollbar {
    display: none;
  }

  .ai-testimonials-card-{{ ai_gen_id }} {
    flex: 0 0 auto;
    width: 300px;
    background-color: {{ block.settings.card_background }};
    border-radius: {{ block.settings.card_border_radius }}px;
    padding: 32px 24px;
    box-shadow: 0 4px 20px rgba(0, 0, 0, 0.1);
    scroll-snap-align: center;
    display: flex;
    flex-direction: column;
    min-height: 200px;
  }

  .ai-testimonials-quote-{{ ai_gen_id }} {
    font-family: {{ settings.type_body_font.family }}, {{ settings.type_body_font.fallback_families }};
    font-size: {{ block.settings.quote_size }}px;
    color: {{ block.settings.card_text_color }};
    line-height: 1.5;
    margin: 0 0 24px;
    flex-grow: 1;
  }

  .ai-testimonials-customer-{{ ai_gen_id }} {
    margin-top: auto;
  }

  .ai-testimonials-name-{{ ai_gen_id }} {
    font-family: {{ settings.type_subheading_font.family }}, {{ settings.type_subheading_font.fallback_families }};
    font-size: {{ block.settings.name_size }}px;
    color: {{ block.settings.card_text_color }};
    font-weight: 600;
    margin: 0 0 8px;
  }

  .ai-testimonials-verified-{{ ai_gen_id }} {
    display: flex;
    align-items: center;
    gap: 6px;
    font-family: {{ settings.type_body_font.family }}, {{ settings.type_body_font.fallback_families }};
    font-size: 12px;
    color: {{ block.settings.verified_color }};
  }

  .ai-testimonials-check-{{ ai_gen_id }} {
    width: 16px;
    height: 16px;
    color: {{ block.settings.verified_color }};
  }

  .ai-testimonials-nav-{{ ai_gen_id }} {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    background: {{ block.settings.arrow_background }};
    border: none;
    border-radius: 50%;
    width: 48px;
    height: 48px;
    display: flex;
    align-items: center;
    justify-content: center;
    cursor: pointer;
    box-shadow: 0 2px 12px rgba(0, 0, 0, 0.15);
    transition: all 0.3s ease;
    z-index: 3;
  }

  .ai-testimonials-nav-{{ ai_gen_id }}:hover {
    background: {{ block.settings.arrow_hover_background }};
    transform: translateY(-50%) scale(1.05);
  }

  .ai-testimonials-nav-left-{{ ai_gen_id }} {
    left: 10px;
  }

  .ai-testimonials-nav-right-{{ ai_gen_id }} {
    right: 10px;
  }

  .ai-testimonials-nav-icon-{{ ai_gen_id }} {
    width: 20px;
    height: 20px;
    color: {{ block.settings.arrow_color }};
  }

  .ai-testimonials-rating-{{ ai_gen_id }} {
    text-align: center;
    display: flex;
    align-items: center;
    justify-content: center;
    gap: 16px;
    flex-wrap: wrap;
  }

  .ai-testimonials-rating-text-{{ ai_gen_id }} {
    font-family: {{ settings.type_subheading_font.family }}, {{ settings.type_subheading_font.fallback_families }};
    font-size: 18px;
    color: {{ block.settings.text_color }};
    font-weight: 600;
  }

  .ai-testimonials-rating-stars-{{ ai_gen_id }} {
    display: flex;
    gap: 4px;
  }

  .ai-testimonials-rating-star-{{ ai_gen_id }} {
    width: 16px;
    height: 16px;
    color: {{ block.settings.star_color }};
  }

  .ai-testimonials-rating-count-{{ ai_gen_id }} {
    font-family: {{ settings.type_body_font.family }}, {{ settings.type_body_font.fallback_families }};
    font-size: 14px;
    color: {{ block.settings.text_color }};
    opacity: 0.8;
  }

