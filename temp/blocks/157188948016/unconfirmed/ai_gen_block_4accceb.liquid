{% endstyle %}

<testimonials-carousel-{{ ai_gen_id }}
  class="ai-testimonials-section-{{ ai_gen_id }}"
  {{ block.shopify_attributes }}
>
  <div class="ai-testimonials-wave-top-{{ ai_gen_id }}"></div>
  
  <div class="ai-testimonials-container-{{ ai_gen_id }}">
    <div class="ai-testimonials-header-{{ ai_gen_id }}">
      <div class="ai-testimonials-stars-{{ ai_gen_id }}">
        {% for i in (1..5) %}
          <svg class="ai-testimonials-star-{{ ai_gen_id }}" viewBox="0 0 24 24" fill="currentColor">
            <path d="M12 2l3.09 6.26L22 9.27l-5 4.87 1.18 6.88L12 17.77l-6.18 3.25L7 14.14 2 9.27l6.91-1.01L12 2z"/>
          </svg>
        {% endfor %}
      </div>
      
      <h1 class="ai-testimonials-title-{{ ai_gen_id }}">{{ block.settings.title }}</h1>
      
      {% if block.settings.subtitle != blank %}
        <p class="ai-testimonials-subtitle-{{ ai_gen_id }}">{{ block.settings.subtitle }}</p>
      {% endif %}
    </div>

    <div class="ai-testimonials-carousel-wrapper-{{ ai_gen_id }}">
      <button class="ai-testimonials-nav-{{ ai_gen_id }} ai-testimonials-nav-left-{{ ai_gen_id }}" aria-label="Previous testimonial">
        <svg class="ai-testimonials-nav-icon-{{ ai_gen_id }}" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
          <polyline points="15,18 9,12 15,6"></polyline>
        </svg>
      </button>

      <div class="ai-testimonials-carousel-{{ ai_gen_id }}">
        {% for i in (1..8) %}
          {% liquid
            assign quote_key = 'testimonial_' | append: i | append: '_quote'
            assign name_key = 'testimonial_' | append: i | append: '_name'
            assign quote = block.settings[quote_key]
            assign name = block.settings[name_key]
          %}
          
          {% if quote != blank and name != blank %}
            <div class="ai-testimonials-card-{{ ai_gen_id }}">
              <p class="ai-testimonials-quote-{{ ai_gen_id }}">"{{ quote }}"</p>
              <div class="ai-testimonials-customer-{{ ai_gen_id }}">
                <h3 class="ai-testimonials-name-{{ ai_gen_id }}">{{ name }}</h3>
                <div class="ai-testimonials-verified-{{ ai_gen_id }}">
                  <svg class="ai-testimonials-check-{{ ai_gen_id }}" viewBox="0 0 24 24" fill="currentColor">
                    <path d="M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z"/>
                  </svg>
                  <span>Verified Customer</span>
                </div>
              </div>
            </div>
          {% endif %}
        {% endfor %}
      </div>

      <button class="ai-testimonials-nav-{{ ai_gen_id }} ai-testimonials-nav-right-{{ ai_gen_id }}" aria-label="Next testimonial">
        <svg class="ai-testimonials-nav-icon-{{ ai_gen_id }}" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
          <polyline points="9,18 15,12 9,6"></polyline>
        </svg>
      </button>
    </div>

    <div class="ai-testimonials-rating-{{ ai_gen_id }}">
      <span class="ai-testimonials-rating-text-{{ ai_gen_id }}">{{ block.settings.rating_text }}</span>
      <div class="ai-testimonials-rating-stars-{{ ai_gen_id }}">
        {% for i in (1..5) %}
          <svg class="ai-testimonials-rating-star-{{ ai_gen_id }}" viewBox="0 0 24 24" fill="currentColor">
            <path d="M12 2l3.09 6.26L22 9.27l-5 4.87 1.18 6.88L12 17.77l-6.18 3.25L7 14.14 2 9.27l6.91-1.01L12 2z"/>
          </svg>
        {% endfor %}
      </div>
      <span class="ai-testimonials-rating-count-{{ ai_gen_id }}">{{ block.settings.total_reviews }}</span>
    </div>
  </div>

  <div class="ai-testimonials-wave-bottom-{{ ai_gen_id }}"></div>
</testimonials-carousel-{{ ai_gen_id }}>

<script>
  (function() {
    class TestimonialsCarousel{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
        this.carousel = this.querySelector('.ai-testimonials-carousel-{{ ai_gen_id }}');
        this.leftButton = this.querySelector('.ai-testimonials-nav-left-{{ ai_gen_id }}');
        this.rightButton = this.querySelector('.ai-testimonials-nav-right-{{ ai_gen_id }}');
        this.cards = this.querySelectorAll('.ai-testimonials-card-{{ ai_gen_id }}');
        this.currentIndex = 0;
        this.cardWidth = 324;
        this.isScrolling = false;
      }

      connectedCallback() {
        this.setupEventListeners();
        this.updateCardWidth();
        this.updateButtonStates();
      }

      setupEventListeners() {
        this.leftButton.addEventListener('click', () => this.scrollLeft());
        this.rightButton.addEventListener('click', () => this.scrollRight());
        
        this.carousel.addEventListener('scroll', () => {
          if (!this.isScrolling) {
            this.updateCurrentIndex();
            this.updateButtonStates();
          }
        });

        let startX = 0;
        let scrollLeft = 0;

        this.carousel.addEventListener('touchstart', (e) => {
          startX = e.touches[0].pageX - this.carousel.offsetLeft;
          scrollLeft = this.carousel.scrollLeft;
        });

        this.carousel.addEventListener('touchmove', (e) => {
          if (!startX) return;
          e.preventDefault();
          const x = e.touches[0].pageX - this.carousel.offsetLeft;
          const walk = (x - startX) * 2;
          this.carousel.scrollLeft = scrollLeft - walk;
        });

        this.carousel.addEventListener('touchend', () => {
          startX = 0;
          this.snapToNearestCard();
        });

        window.addEventListener('resize', () => {
          this.updateCardWidth();
        });
      }

      updateCardWidth() {
        if (this.cards.length > 0) {
          const cardRect = this.cards[0].getBoundingClientRect();
          this.cardWidth = cardRect.width + 24;
        }
      }

      scrollLeft() {
        if (this.currentIndex > 0) {
          this.currentIndex--;
          this.scrollToIndex(this.currentIndex);
        }
      }

      scrollRight() {
        if (this.currentIndex < this.cards.length - 1) {
          this.currentIndex++;
          this.scrollToIndex(this.currentIndex);
        }
      }

      scrollToIndex(index) {
        this.isScrolling = true;
        const scrollPosition = index * this.cardWidth;
        this.carousel.scrollTo({
          left: scrollPosition,
          behavior: 'smooth'
        });
        
        setTimeout(() => {
          this.isScrolling = false;
          this.updateButtonStates();
        }, 300);
      }

      updateCurrentIndex() {
        const scrollLeft = this.carousel.scrollLeft;
        this.currentIndex = Math.round(scrollLeft / this.cardWidth);
      }

      snapToNearestCard() {
        this.updateCurrentIndex();
        this.scrollToIndex(this.currentIndex);
      }

      updateButtonStates() {
        this.leftButton.style.opacity = this.currentIndex === 0 ? '0.5' : '1';
        this.rightButton.style.opacity = this.currentIndex >= this.cards.length - 1 ? '0.5' : '1';
        
        this.leftButton.disabled = this.currentIndex === 0;
        this.rightButton.disabled = this.currentIndex >= this.cards.length - 1;
      }
    }

    customElements.define('testimonials-carousel-{{ ai_gen_id }}', TestimonialsCarousel{{ ai_gen_id }});
  })();
</script>

