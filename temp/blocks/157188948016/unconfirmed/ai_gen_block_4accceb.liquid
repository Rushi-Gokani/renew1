      padding: 60px 0;
    }

    .ai-testimonials-container-{{ ai_gen_id }} {
      padding: 0 16px;
    }

    .ai-testimonials-header-{{ ai_gen_id }} {
      margin-bottom: 40px;
    }

    .ai-testimonials-title-{{ ai_gen_id }} {
      font-size: {{ block.settings.title_size | times: 0.65 }}px;
      line-height: 1.2;
      margin-bottom: 12px;
      padding: 0 8px;
    }

    .ai-testimonials-subtitle-{{ ai_gen_id }} {
      font-size: {{ block.settings.subtitle_size | times: 0.9 }}px;
      padding: 0 8px;
    }

    .ai-testimonials-carousel-{{ ai_gen_id }} {
      padding: 0 20px;
    }

    .ai-testimonials-card-{{ ai_gen_id }} {
      width: 260px;
    }

    .ai-testimonials-nav-{{ ai_gen_id }} {
      width: 40px;
      height: 40px;
    }

    .ai-testimonials-nav-icon-{{ ai_gen_id }} {
      width: 16px;
      height: 16px;
    }

    .ai-testimonials-nav-left-{{ ai_gen_id }} {
      left: 5px;
    }

    .ai-testimonials-nav-right-{{ ai_gen_id }} {
      right: 5px;
    }

    .ai-testimonials-rating-{{ ai_gen_id }} {
      gap: 12px;
    }

    .ai-testimonials-rating-text-{{ ai_gen_id }} {
      font-size: 16px;
    }
  }

  @media screen and (max-width: 480px) {
    .ai-testimonials-section-{{ ai_gen_id }} {
      padding: 50px 0;
    }

    .ai-testimonials-container-{{ ai_gen_id }} {
      padding: 0 12px;
    }

    .ai-testimonials-header-{{ ai_gen_id }} {
      margin-bottom: 32px;
    }

    .ai-testimonials-title-{{ ai_gen_id }} {
      font-size: {{ block.settings.title_size | times: 0.5 }}px;
      line-height: 1.3;
      margin-bottom: 10px;
      padding: 0 4px;
      max-width: 100%;
    }

    .ai-testimonials-subtitle-{{ ai_gen_id }} {
      font-size: {{ block.settings.subtitle_size | times: 0.85 }}px;
      padding: 0 4px;
      line-height: 1.4;
    }

    .ai-testimonials-stars-{{ ai_gen_id }} {
