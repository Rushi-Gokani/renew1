    .ai-testimonials-card-{{ ai_gen_id }} {
      width: 280px;
    }

    .ai-testimonials-carousel-{{ ai_gen_id }} {
      padding: 0 40px;
    }

    .ai-testimonials-title-{{ ai_gen_id }} {
      font-size: {{ block.settings.title_size | times: 0.8 }}px;
    }
  }

  @media screen and (max-width: 768px) {
